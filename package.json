{
    "name": "java-generate-setters-getters",
    "displayName": "java-generate-setters-getters",
    "description": "Java Generate Setters and Getters",
    "version": "1.0.0",
    "publisher": "sohibe",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:extension.javaGenerateSettersGetters"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.javaGenerateSettersGetters",
                "title": "Java Generate Setters and Getters"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}